generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model WardrobeItem {
  id        String   @id @default(cuid())
  title     String
  category  String
  imageUrl  String
  publicId  String
  createdAt DateTime @default(now())
}
